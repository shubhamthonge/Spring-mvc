<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="a" class="com.tech.Address">
	<property name="cityname"  value="pune"></property>
	<property name="pincode" value="121238"></property>
	</bean>
	
	<bean id ="e" class="com.tech.Employee" >
	<property name="id"  value="123"></property>
	<property name="name" value="shubhsm"></property>
	<property name="address" ref="a"></property>
	</bean>
</beans>