<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="a1" class="com.tech.Address">
	<property name="cityname" value="jaipur"></property>
	<property name="pincode" value="59638"></property>
	</bean>
	
	<bean id="e1" class="com.tech.Employee">
	<property name="id" value="232"></property>
	<property name="name" value="pankaj"></property>
	<property name="address" ref="a1"></property>
	</bean>
</beans>